<?xml version="1.0" encoding="utf-8"?><!--$A-->
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".view.fragments.ConceptFragment"
    android:layout_margin="8dp"> <!--$A7-->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#EAE7E7"><!--$A8-->
        <ImageView
            android:id="@+id/image_concept_function"
            android:layout_width="102dp"
            android:layout_height="101dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:src="@drawable/ic_software"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" /><!--$A13-->
        <TextView
            android:id="@+id/concept_name_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="@+id/ask_button"
            app:layout_constraintStart_toStartOf="@+id/ask_button"
            app:layout_constraintTop_toBottomOf="@+id/image_concept_function" /><!--$A8-->
        <Button
            android:id="@+id/ask_button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="Ask"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/concept_name_text_view" /><!--$A13-->
    </androidx.constraintlayout.widget.ConstraintLayout><!--$A-->
</FrameLayout><!--$A-->